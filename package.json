{
  "name": "floumy-api",
  "version": "0.0.1",
  "description": "The API for the Floumy app",
  "author": "Floumy",
  "private": true,
  "license": "Proprietary",
  "scripts": {
    "build": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run test-db:up && jest --runInBand && npm run test-db:down",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "npm run test-db:up && jest --coverage --runInBand && npm run test-db:down",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test-db:up && jest --config ./test/jest-e2e.json --runInBand && npm run test-db:down",
    "test-db:up": "[ -z \"$(docker ps -a -q -f name=test-db)\" ] && docker run --name test-db -e POSTGRES_USER=testuser -e POSTGRES_PASSWORD=testpass -e POSTGRES_DB=testdb -p 5455:5432 -d postgres:13 || echo 'Container test-db already exists'",
    "test-db:down": "docker stop test-db && docker rm test-db",
    "typeorm": "typeorm-ts-node-commonjs",
    "stripe:forward": "stripe listen --forward-to localhost:8080/payments/webhook",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org floumy --project node-nestjs ./dist && sentry-cli sourcemaps upload --org floumy --project node-nestjs ./dist"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@sentry/cli": "^2.36.6",
    "@sentry/nestjs": "^8.33.1",
    "@sentry/profiling-node": "^8.33.1",
    "@types/stripe-v3": "^3.1.33",
    "aws-sdk": "^2.1541.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "pg": "^8.11.3",
    "postmark": "^4.0.2",
    "rxjs": "^7.8.1",
    "stripe": "^15.8.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/main.ts",
      "/src/config/",
      "\\.module\\.ts$",
      "\\.spec\\.ts$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}